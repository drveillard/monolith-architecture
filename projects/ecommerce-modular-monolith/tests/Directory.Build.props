<!-- https://docs.microsoft.com/en-us/visualstudio/msbuild/customize-your-build -->
<Project>
  <PropertyGroup>
      <TargetFramework>net6.0</TargetFramework>
      <LangVersion>latest</LangVersion>
      <Nullable>enable</Nullable>
      <ImplicitUsings>enable</ImplicitUsings>
	  <NoWarn>CS1587,CS1591,CS1998,NU5105</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.VisualStudio.TestPlatform" Version="14.0.0"/>
    <PackageReference Include="Mongo2Go" Version="3.1.3"/>
    <PackageReference Include="Respawn" Version="5.0.1"/>
    <PackageReference Include="Squadron.Mongo" Version="0.13.0"/>
    <PackageReference Include="xunit" Version="2.4.1"/>
    <PackageReference Include="FluentAssertions" Version="6.1.0"/>
    <PackageReference Include="FluentAssertions.Web" Version="1.1.2"/>
    <PackageReference Include="FluentResults.Extensions.FluentAssertions" Version="0.6.0"/>
    <PackageReference Include="MartinCostello.Logging.XUnit" Version="0.1.1"/>
    <PackageReference Include="Microsoft.AspNetCore.Mvc.Testing" Version="6.0.4"/>
    <PackageReference Include="Microsoft.NET.Test.Sdk" Version="17.1.0"/>
    <PackageReference Include="NSubstitute" Version="4.2.2"/>
    <PackageReference Include="Serilog.Sinks.Debug" Version="2.0.0"/>
    <PackageReference Include="Serilog.Sinks.Xunit2" Version="1.1.0"/>
    <PackageReference Include="System.Reactive.Linq" Version="5.0.0"/>
    <PackageReference Include="AutoBogus" Version="2.13.1" />
    <PackageReference Include="AutoBogus.NSubstitute" Version="2.13.1" />
    <PackageReference Include="MartinCostello.Logging.XUnit" Version="0.2.0"/>
    <PackageReference Include="xunit.analyzers" Version="0.10.0"/>
  </ItemGroup>
  
 <ItemGroup Label="Implicit usings">
    <Using Include="FluentAssertions" />
    <Using Include="NSubstitute" />
    <Using Include="Xunit" />
  </ItemGroup>
  
</Project>